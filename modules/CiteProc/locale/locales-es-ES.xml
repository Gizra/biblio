   <date form="text">
      <date-part name="day" suffix=" de "/>
      <date-part name="month" suffix=" de "/>
      <date-part name="year"/>
   </date>
   <date form="numeric">
      <date-part name="day" form="numeric-leading-zeros" suffix="/"/>
